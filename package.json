{
  "name": "domfs-findit",
  "version": "2.0.1",
  "description": "Walk a directory tree in DOM File System",
  "main": "legacy/findit.js",
  "dependencies": {
    "emitify": "^3.1.0",
    "webpack-cli": "^2.0.10"
  },
  "devDependencies": {
